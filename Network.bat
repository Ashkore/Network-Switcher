netsh interface show interface | findstr Disabled | FINDSTR /C:"Ethernet" >nul & IF ERRORLEVEL 1 (ECHO "Disabling Etherenet, Enabling Wi-Fi" &netsh interface set interface "Wi-Fi" admin=enable & netsh interface set interface "Ethernet" admin=disable) else (ECHO "Disabling Wi-Fi, Enabling Etherent" & netsh interface set interface "Ethernet" admin=enable & netsh interface set interface "Wi-Fi" admin=disable)